{
  "name": "cryptonia-server",
  "type": "commonjs",
  "scrips": {
    "dev": "nodemon server.js",
    "start": "env NODE_ENV=production && pm2 start server.js",
    "restart": "env NODE_ENV=production && pm2 restart server.js",
    "stop": "pm2 stop server.js",
    "status": "pm2 status",
    "logs": "pm2 logs"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "pm2": "^5.4.3"
  },
  "devDependencies": {
    "nodemon": "^3.1.9"
  }
}
